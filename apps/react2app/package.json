{
  "name": "react2app",
  "version": "0.1.5",
  "type": "module",
  "bin": {
    "react2app": "./dist/bin/index.js"
  },
  "exports": {
    ".": "./dist/src/components/index.js"
  },
  "types": "./dist/src/components/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "pnpm copy-data && tsc -w ",
    "start": "node dist/bin/index.js",
    "copy-data": "pnpm copy-templates && pnpm copy-fastlane",
    "copy-templates": "copyfiles -u 3 \"src/cli/templates/**/*\" dist/templates",
    "copy-fastlane": "copyfiles -u 3 \"src/cli/fastlane/**/*\" dist/fastlane",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@react2app/stack-navigation": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer-autocomplete-prompt": "^3.0.3",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.3.0",
    "classnames": "^2.5.1",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "get-port": "^7.1.0",
    "inquirer": "^9.2.12",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "next": "^15.0.3",
    "ora": "^8.1.1",
    "progress": "^2.0.3",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@react2app/typescript-config": "workspace:*",
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/progress": "^2.0.7",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.0"
  }
}
